            font-size-adjust: none;
            font-style: normal;
            font-variant: normal;
            font-weight: normal;
            line-height: normal;
            padding-top: 2px;
            text-decoration: none;
        }

        .ng-invalid.ng-dirty {
            border-color: #FA787E;
        }

        .ng-touched.ng-invalid {
            border-color: #FA787E;
        }

        .ng-touched.ng-pristine {
            border-color: #FA787E;
        }
    </style>
